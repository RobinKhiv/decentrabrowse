    "@mui/material": "^5.15.10",
    "electron": "^28.2.3",
    "ethers": "^6.13.5",
    "https-browserify": "^1.0.0",
    "ipfs-http-client": "^56.0.1",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",

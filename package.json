{
  "name": "decentrabrowse",
  "version": "1.0.0",
  "description": "A privacy-focused blockchain browser",
  "main": "dist/main/main.js",
  "scripts": {
    "start": "electron .",
    "build": "webpack --config webpack.main.config.js && webpack --config webpack.renderer.config.js && webpack --config webpack.preload.config.js",
    "dev": "concurrently \"npm run build:watch\" \"electron .\"",
    "build:watch": "webpack --config webpack.main.config.js --watch & webpack --config webpack.renderer.config.js --watch & webpack --config webpack.preload.config.js --watch",
    "copy-files": "mkdir -p dist/renderer && cp -r src/renderer/index.html dist/renderer/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
